<Window x:Class="Dacha.Inspector.DictionaryAddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dacha.Inspector"
        xmlns:dictionaries="clr-namespace:Dacha.Inspector.Dictionaries"
        mc:Ignorable="d"
        Title="DictionaryAddWindow" Height="300" Width="300"
        d:DataContext="{d:DesignInstance Type=dictionaries:DictionaryAddViewModelDummy, IsDesignTimeCreatable=True}">
    <StackPanel>
        <ContentControl Content ="{Binding DictionaryContent}"/>
        <StackPanel Orientation="Horizontal">
            <Button IsDefault="True">OK</Button>
            <Button IsCancel="True">Cancel</Button>
        </StackPanel>
    </StackPanel>
</Window>
