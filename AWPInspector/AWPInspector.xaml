<Window x:Class="bypasser.AWPInspector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bypasser"
        mc:Ignorable="d"
        Title="Inspector" Height="350" Width="525"
        DataContext="{StaticResource AWPInspectorViewModel}">
    <DockPanel>
        <Menu DockPanel.Dock="Top" >
            <MenuItem Header="_Common">
                <MenuItem Header="Sectors" Command="{Binding SectorCommand}"/>
                <MenuItem Header="Places" Command="{Binding PlacesCommand}"/>
                <MenuItem Header="Item1">Item1</MenuItem>
                <MenuItem Header="Item1">Item1</MenuItem>
            </MenuItem>
            <MenuItem Header="_Exit" Command="{Binding ExitCommand}">
            </MenuItem>
        </Menu>
    </DockPanel>
</Window>
